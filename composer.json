{
  "name": "novembit/i18n",
  "description": "Standalone i18n.",
  "keywords": [
    "i18n",
    "i10n"
  ],
  "license": "GPL-3.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "library",
  "require": {
    "php": ">=7.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-mbstring": "*",
    "yiisoft/yii2": "~2.0.14",
    "google/cloud-translate": "*",
    "masterminds/html5": "^2.0",
    "cache/cache": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "NovemBit\\i18n\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
