{
  "name": "novembit/i18n",
  "description": "Standalone i18n.",
  "keywords": [
    "i18n",
    "i10n"
  ],
  "license": "GPL-3.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "library",
  "require": {
    "php": ">=7.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-simplexml": "*",
    "ext-mbstring": "*",
    "psr/cache": "*",
    "psr/log": "*",
    "phpfastcache/phpfastcache": "*",
    "google/cloud-translate": "*",
    "masterminds/html5": "^2.7",
    "monolog/monolog": "*",
    "doctrine/dbal": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "phpmd/phpmd": "*",
    "squizlabs/php_codesniffer": "*",
    "friendsofphp/php-cs-fixer": "*"
  },
  "autoload": {
    "psr-4": {
      "NovemBit\\i18n\\": "src/",
      "NovemBit\\tests\\i18n\\": "tests/"
    }
  },
  "scripts": {
    "style": "phpcs --standard=PSR12"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
